using System;
					
public class Program
{
	public static void Main()
	{
		int money = int.Parse(Console.ReadLine());
		if(money >= 100)
		{
			Console.WriteLine("You can survive a day.");
		}
		else
			Console.WriteLine("You are broke lmao.");
	}
}


//else if below

using System;
					
public class Program
{
	public static void Main()
	{
		int am;
		Console.WriteLine("Combien ces vinyl disque toi as?");
		am = int.Parse(Console.ReadLine());
		if (am >=100)
			Console.Write("Toi es ce professionale collecteur!");
		else if (am >= 50 && am < 99)
			Console.Write("Toi es la fuo collectur!");
		else if (am >= 20 && am < 49)
			Console.Write("Toi es la meilleur collecteur.");
		else if (am >= 10 && am < 19)
		    Console.Write("Toi es la jeuneur collecteur.");
		else if (am >= 1 && am < 9 )
			Console.Write("Toi envie plus les disques.");
		else
			Console.Write("Pourquoi....");
	}
}


// Options

using System;
					
public class Program
{
	public static void Main()
	{
		Console.WriteLine("Please select your desired artist record to collect.");
		Console.WriteLine("Type 1 for The Beatles");
		Console.WriteLine("Type 2 for Françoise Hardy");
		Console.WriteLine("Type 3 for Radiohead");
		
		int num = int.Parse(Console.ReadLine());
		
		if(num == 1){
			Console.WriteLine("You have picked The Beatles, It won't be long! ");
		}
		else if(num == 2){
			Console.WriteLine("You have picked Françoise Hardy, comme garçons!");
		}
		else if(num == 3){
			Console.WriteLine("You have picked Radiohead, You do it to yourself, just you, you and no one else!");
		}
		else
			Console.WriteLine("Well you either don't like any of these or you just bland taste in music...");
	}
}
