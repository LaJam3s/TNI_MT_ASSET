using System;
					
public class Program
{
	public static void Main()
	{
		float num1,num2,num3,total;
		Console.WriteLine("********************************************");
		Console.Write("Number 1 = ");
		num1 = float.Parse(Console.ReadLine());
		Console.Write("Number 2 = ");
		num2 = float.Parse(Console.ReadLine());
		Console.Write("Number 3 = ");
		num3 = float.Parse(Console.ReadLine());
		Console.WriteLine("********************************************");
		total = num1 + num2 + num3;
		Console.WriteLine("Summary of {0}, {1} et {2} est {3}",num1,num2,num3,total);
		Console.WriteLine("********************************************");
		Console.WriteLine("Average of {0} / 3 is {1:F}",total,total/3);

 // This code uses average dividing 3 and plusing
using System;
					
public class Program
{
	public static void Main()
	{
		float salary, social, tax;
		Console.WriteLine("------------------------------------------------------------------");
		Console.Write("Input employee salary : ");
		salary = float.Parse(Console.ReadLine());
		Console.WriteLine("\n------------------------------------------------------------------");
		Console.WriteLine("Employee salary {0,20:N} Baht ",salary);
		social = salary * 3f / 100f;
		tax = salary * 4f / 100f;
		Console.WriteLine("Social {0,36:N} Baht\nTax {1,40:N} Baht",social,tax);
		Console.WriteLine("------------------------------------------------------------------");
		Console.WriteLine("Salary {0,35:N} Baht",salary - social - tax);
		// They want to tax you!!!
	}
}

