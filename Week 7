using System;

public class Program
{
  public static void Main()
  {
	  
    int x = 0; //Loop Intiate
    while(x < 5){ //Count to 5
      x++; //Check loop
      Console.WriteLine("'{0}', '{1}'",x,"Valinkorn"); //{0},{1} made for tagging 
	  Console.Write("Inpu tname :");
	  string name = Console.ReadLine(); //making it so asks 5 times
    }
    Console.WriteLine("Loop is Done");
  }
}

